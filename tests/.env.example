# BingX API Test Configuration
# Copy this file to .env and fill in your actual API credentials

# Required: Your BingX API Key and Secret
BINGX_API_KEY=your_api_key_here
BINGX_API_SECRET=your_api_secret_here

# Optional: Custom base URI (defaults to official BingX API)
BINGX_BASE_URI=https://open-api.bingx.com

# Test symbols (modify if needed)
BINGX_TEST_SYMBOL=BTC-USDT
BINGX_TEST_SYMBOL_SPOT=BTC-USDT

# Test configuration
BINGX_TEST_LEVERAGE=10
BINGX_TEST_MARGIN=100
BINGX_TEST_QUANTITY=0.001
